{
    "name": "dontastaturo/laravel-api-consumer",
    "description": "Awesome Laravel Api Consumer",
    "keywords": [
        "blackbits",
        "laravel api consumer",
        "laravel",
        "api consumer"
    ],
    "homepage": "https://github.com/black-bits/larave-api-consumer",
    "license": "MIT",
    "authors": [{
            "name": "Oliver Heck",
            "email": "oliver.heck@blackbits.io",
            "homepage": "https://blackbits.io",
            "role": "Developer"
        },
        {
            "name": "Andreas Przywara",
            "email": "andreas.przywara@blackbits.io",
            "homepage": "https://blackbits.io",
            "role": "Developer"
        },
        {
            "name": "Adrian Raeuchle",
            "email": "adrian.raeuchle@blackbits.io",
            "homepage": "https://blackbits.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "kitetail/zttp": "^0.3.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "BlackBits\\ApiConsumer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlackBits\\ApiConsumer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackBits\\ApiConsumer\\ApiConsumerServiceProvider"
            ]
        }
    }
}